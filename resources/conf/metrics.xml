<?xml version="1.0" encoding="UTF-8"?>
<CellHealthMetrics>
    <PMIStatsType unit="true" unitseparator="_" separateMetric="false"> <!--unit true -> concatenar el tipo de unidad al nombre de metrica con uniqueseparator _ -->
        <CountStatistic count="true" />
        <DoubleStatisc count="true"/>
        <AverageStatistic count="true" total="false" min="true" max="false"/>
        <BoundaryStatistic upperBound="true" lowebBound="false" />
        <RangeStatistic highWaterMark="true" lowWaterMark="false" current="true" integral="false"/>
        <TimeStatitic count="true" total="false" min="true" max="false"/>
        <BoundedRangeStatistic upperBound="true" lowebBound="false" highWaterMark="true" lowWaterMark="false" current="true" integral="false"/>
<!--        #TimeStatitic = AverageStatistic
        #BoundedRangeStatistic= BoundaryStatisc + RangeStatictic
        Por defecto sino se especifica nada es = a la suma de configuraciones
        -->
    </PMIStatsType>
    <MetricGroup>
        <StatsType>jvmRuntimeModule</StatsType>
        <Prefix>jvm</Prefix>
        <UniqueInstance>true</UniqueInstance>
        <Metrics>
            <Metric id="1">
                <Name>HeapSize</Name>
                <Scale>1024</Scale>
            </Metric>
            <Metric id="3">
                <Name>Used</Name>
                <Scale>1024</Scale>
            </Metric>
            <Metric id="5">
                <Name>CpuUsage</Name>
            </Metric>
        </Metrics>
    </MetricGroup>
    <MetricGroup>
        <StatsType>threadPoolModule</StatsType>
        <AllowGlobal>true</AllowGlobal>
        <Prefix>Thrpool</Prefix>
        <UniqueInstance>false</UniqueInstance>
        <InstanceFilter>SoapConnectorThreadPool,SIBFAPThreadPool</InstanceFilter>
        <Metrics>
            <Metric id="3"> ID NONULL
                <Name>ActiveCount</Name>
            </Metric>
            <Metric id="4">
                <Name>PoolSize</Name>
            </Metric>
            <Metric id="8">
                <Name>ConcurrentHungThreadCount</Name>
            </Metric>
        </Metrics>
    </MetricGroup>
    <MetricGroup>
        <StatsType>transactionModule</StatsType>
        <AllowGlobal>true</AllowGlobal>
        <Prefix>transaction</Prefix>
        <UniqueInstance>false</UniqueInstance>
        <Metrics>
            <Metric id="4">
                <Name>ActiveCount</Name>
            </Metric>
        </Metrics>
    </MetricGroup>
    <MetricGroup>
        <StatsType>connectionPoolModule</StatsType>
        <AllowGlobal>true</AllowGlobal>
        <Prefix>conpool</Prefix>
        <UniqueInstance>false</UniqueInstance>
        <Metrics>
            <Metric id="5">
                <Name>PoolSize</Name>
            </Metric>
            <Metric id="6">
                <Name>FreePoolSize</Name>
            </Metric>
            <Metric id="7">
                <Name>WaitingThreadCount</Name>
            </Metric>
        </Metrics>
    </MetricGroup>
    <MetricGroup>
        <StatsType>j2cModule</StatsType>
        <AllowGlobal>true</AllowGlobal>
        <Prefix>j2c</Prefix>
        <UniqueInstance>false</UniqueInstance>
        <Metrics>
            <Metric id="7"/>
        </Metrics>
    </MetricGroup>
    <MetricGroup>
        <StatsType>servletSessionsModule</StatsType>
        <AllowGlobal>true</AllowGlobal>
        <Prefix>servlets</Prefix>
        <UniqueInstance>false</UniqueInstance>
        <Metrics>
            <Metric id="7">
                <Name>ConcurrentSessions</Name>
            </Metric>
        </Metrics>
    </MetricGroup>
    <MetricGroup>
        <StatsType>webAppModule</StatsType>
        <Prefix>webapp</Prefix>
        <UniqueInstance>true</UniqueInstance>
        <Metrics>
            <Metric id="13"/>
        </Metrics>
    </MetricGroup>
</CellHealthMetrics>